{
  "ssh-clone-pattern": "git@github.com:%s.git",
  "https-clone-pattern": "https://github.com/%s.git",
  "api-url": "https://api.github.com",
  "repos": {
    "ninja": {
      "remote": {
        "id": "ninja-build/ninja"
      }
    },
    "catch": {
      "remote": {
        "id": "philsquared/Catch"
      }
    }
  },
  "default-branch-scheme": "develop",
  "branch-schemes": {
    "master": {
      "versions": {
        "ninja": "1.7.2",
        "catch": "1.9.5"
      }
    },
    "develop": {
      "versions": {
        "ninja": "1.7.2",
        "catch": "1.9.5"
      }
    }
  }
}
